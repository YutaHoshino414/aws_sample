<div class="">
  <ul class="gnav">
    <% @parents.each do |parent| %>
      <li><a><%= parent.name %></a>
        <ul>
        <% parent.children.each do |child| %>
          <li><a><%= child.name %></a>
            <ul>
            <% child.children.each do |grandchild| %>
              <li><a><%= grandchild.name %></a></li>
            <% end %>
            </ul>
          </li>
          <% end %>
          <li><a href="">Child2</a></li>
          <li><a href="">Child3</a></li>
          <li><a href="">Child4</a></li>
          <li><a href="">Child5</a></li>
        </ul>
      </li>
      <% end %>
      <li><a href="">キッズ</a></li>
  </ul>
</div>
